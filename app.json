{
  "expo": {
    "name": "My NFT",
    "slug": "new_react_native_app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/effect.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/nft.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/nft.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.mahesh.raddyx.new_react_native_app"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
